        <class_source><![CDATA[
    /**
     * source: {@link com.mozilla.bugzilla_etl.di.steps.ImportFlagsStep}
     *
     * Input step(s):
     *    * "Flag Types": A stream of (ID, NAME) flags from BMO.
     * Output step(s):
     *    * A stream of (ID, NAME, STATUS) flags for Lily.
     */
    private static final String IN_STEP = "Flag Types";
    private com.mozilla.bugzilla_etl.di.FlagDestination destination;
    private com.mozilla.bugzilla_etl.di.ImporterFlagSource source;
    private int count = 0;

    public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
        if (first) {
            first = false;
            final RowMeta outputRowMeta = new RowMeta();
            data.outputRowMeta = outputRowMeta;
            meta.getFields(data.outputRowMeta, getStepname(), null, null, parent);
            source = new com.mozilla.bugzilla_etl.di.ImporterFlagSource(this, findInputRowSet(IN_STEP));
            destination = new com.mozilla.bugzilla_etl.di.FlagDestination (this, outputRowMeta);
        }
        if (!source.hasMore()) {
            setOutputDone();
            System.out.print("DONE (have read " + count + " flags from BMO).\n");
            return false;
        }
        destination.send(source.receive());
        ++count;
        System.out.print("Read " + count + " flags from BMO.\n");
        return true;
    }
]]></class_source>
