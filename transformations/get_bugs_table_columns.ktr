  <step>
    <name>Get bugs table columns</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>bugs</connection>
    <sql>SELECT COLUMN_NAME
FROM INFORMATION_SCHEMA.COLUMNS
WHERE TABLE_SCHEMA=&apos;bugs&apos;
AND TABLE_NAME=&apos;bugs&apos;
AND COLUMN_NAME NOT IN (&apos;bug_id&apos;,&apos;delta_ts&apos;,&apos;lastdiffed&apos;,&apos;creation_ts&apos;,&apos;reporter&apos;,&apos;assigned_to&apos;,&apos;qa_contact&apos;,&apos;product_id&apos;,&apos;component_id&apos;);</sql>
    <limit>0</limit>
