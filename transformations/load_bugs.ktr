    <connection>bugs</connection>
    <sql>SELECT
    `bug_id` AS activity_bug_id,
    `modifier`.`login_name` AS modified_by,
    `bug_when`,
    IF(`name` = &apos;flagtypes.name&apos; OR `name` LIKE &apos;cf_%&apos;, &apos;flags&apos;, field.`name`) AS field_name,
    CASE
        WHEN `name` IN (&apos;resolution&apos;, &apos;status_whiteboard&apos;, &apos;target_milestone&apos;) AND `removed` = &apos;&apos; THEN &apos;&lt;empty&gt;&apos;
        WHEN `name` IN (&apos;flags&apos;, &apos;keywords&apos;)                       AND `removed` IS NULL  THEN &apos;_&apos;
        WHEN `name` LIKE &apos;cf_%&apos;                                    AND `removed` =  &apos;---&apos; THEN &apos;_&apos;
        WHEN `name` LIKE &apos;cf_%&apos; AND LENGTH(activity.`removed`) = 1 AND `removed` &lt;&gt; &apos;---&apos; THEN CONCAT(field.`description`,      `removed`)
        WHEN `name` LIKE &apos;cf_%&apos; AND LENGTH(activity.`removed`) &gt; 1 AND `removed` &lt;&gt; &apos;---&apos; THEN CONCAT(field.`description`, &apos;-&apos;, `removed`)
        ELSE IF(`removed` IS NULL or `removed` = &apos;&apos;, &apos;_&apos;, `removed`)
    END AS removed,
    CASE
        WHEN `name` IN (&apos;resolution&apos;, &apos;status_whiteboard&apos;, &apos;target_milestone&apos;) AND `added` = &apos;&apos; THEN &apos;&lt;empty&gt;&apos;
        WHEN `name` IN (&apos;flags&apos;, &apos;keywords&apos;)                     AND `added` IS NULL  THEN &apos;_&apos;
        WHEN `name` LIKE &apos;cf_%&apos;                                  AND `added` =  &apos;---&apos; THEN &apos;_&apos;
        WHEN `name` LIKE &apos;cf_%&apos; AND LENGTH(activity.`added`) = 1 AND `added` &lt;&gt; &apos;---&apos; THEN CONCAT(field.`description`,      `added`)
        WHEN `name` LIKE &apos;cf_%&apos; AND LENGTH(activity.`added`) &gt; 1 AND `added` &lt;&gt; &apos;---&apos; THEN CONCAT(field.`description`, &apos;-&apos;, `added`)
        ELSE IF(`removed` IS NULL or `added` = &apos;&apos;, &apos;_&apos;, `added`)
    END AS added,
    `attach_id`,
    CAST(DATE(`bug_when`)+0 AS SIGNED INT) AS modified_on,
    CAST(TIME(`bug_when`)+0 AS SIGNED INT) AS modified_at
FROM
    `sanitized_bugs_activity` activity
    JOIN `fielddefs` field ON activity.`fieldid` = field.`id`
    JOIN `profiles` modifier ON `who` = modifier.`userid`
WHERE
    `bug_when` &gt; ?
    AND ${BUG_IDS_PARTITION}
    AND (
        field.`name` IN (&apos;assigned_to&apos;, &apos;bug_severity&apos;, &apos;bug_status&apos;, &apos;component&apos;, &apos;flagtypes.name&apos;,
                         &apos;keywords&apos;, &apos;bug_group&apos;, &apos;op_sys&apos;, &apos;priority&apos;, &apos;product&apos;, &apos;resolution&apos;, &apos;status_whiteboard&apos;,
                         &apos;target_milestone&apos;, &apos;version&apos;)
        OR (field.`name` LIKE &apos;cf_%&apos; AND field.`name` NOT IN (&apos;cf_colo_site&apos;, &apos;cf_crash_signature&apos;, &apos;cf_tracking_thunderbird6&apos;, &apos;cf_tracking_thunderbird7&apos;, &apos;cf_status_thunderbird6&apos;, &apos;cf_status_thunderbird7&apos;))
    )
    AND `attach_id` IS NULL
ORDER BY
    `bug_id`,
    `bug_when` DESC,
    modified_by</sql>
