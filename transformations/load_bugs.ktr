        <jsScript_script>var startDateTime;

var bugIDsPartition = getVariable(&quot;BUG_IDS_PARTITION&quot;, &quot;1 = 1&quot;);
var overrideStartTime = getVariable(&quot;OVERRIDE_START_TIME&quot;, &quot;&quot;);

if (overrideStartTime.length == 19) {
	startDateTime = str2date(overrideStartTime, &quot;yyyy-MM-dd HH:mm:ss&quot;);
} else {

    &#47;&#47; NEED TO GET THIS FROM LILY INSTEAD
	var strConn = &quot;bugs_dw&quot;;
	var strSQL = &quot;SELECT MAX(valid_from_ts) FROM FACT_bug_states WHERE &quot;+bugIDsPartition.replace(&quot;bug_id&quot;, &quot;bug_nk&quot;);
	var xArr = fireToDB(strConn, strSQL);

	startDateTime = xArr[0][0];
	if (startDateTime == null) {
		startDateTime = str2date(&quot;1900-01-01&quot;, &quot;yyyy-MM-dd&quot;);
	} else {
		dateAdd(startDateTime, &quot;ss&quot;, 1);
	}
}

writeToLog(&quot;m&quot;, &quot;Updating FACT_bug_states &quot;+bugIDsPartition+&quot; starting from [&quot;+date2str(startDateTime,&quot;yyyy-MM-dd HH:mm:ss&quot;)+&quot;]&quot;);
</jsScript_script>
