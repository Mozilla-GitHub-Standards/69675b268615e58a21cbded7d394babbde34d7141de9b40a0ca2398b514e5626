    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>MarkFirstLastRow</class_name>

        <class_source><![CDATA[import java.util.*;

private long rowNumber = 0;
private Long prevBugID = null;
private Object[] previousRow = null;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    if (first)
    {
        first = false;

		previousRow = getRow();
		if (previousRow == null) {
			setOutputDone();
			return false;
		}
        data.outputRowMeta = getInputRowMeta().clone();
        meta.getFields(data.outputRowMeta, getStepname(), null, null, parent);

		prevBugID = get(Fields.In, "activity_bug_id").getInteger(previousRow);
        get(Fields.Out, "is_latest_state").setValue(previousRow, prevBugID);

		return true;
    }

	Object[] r = getRow();

	if (r == null) {
		outputPreviousRowRow(true);
		setOutputDone();
		return false;
	}

	Long currBugID = get(Fields.In, "activity_bug_id").getInteger(r);
	if (!prevBugID.equals(currBugID)) {
		outputPreviousRowRow(true);
        get(Fields.Out, "is_latest_state").setValue(r, currBugID);
	} else {
		outputPreviousRowRow(false);
	}

	previousRow = r;
	prevBugID = currBugID;
	return true;
}

private void outputPreviousRowRow(final boolean isEarliest) throws KettleException
{
	if (isEarliest) {
        get(Fields.Out, "is_earliest_state").setValue(previousRow, prevBugID);
	}
    get(Fields.Out, "row_number").setValue(previousRow, Long.valueOf(Math.abs(--rowNumber)));
	putRow(data.outputRowMeta, previousRow);
}

]]></class_source>
