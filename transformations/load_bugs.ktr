    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>var startDateTime;

var bugIDsPartition = getVariable(&quot;BUG_IDS_PARTITION&quot;, &quot;1 = 1&quot;);
var overrideStartTime = getVariable(&quot;OVERRIDE_START_TIME&quot;, &quot;&quot;);
var lastModificationTime = getVariable(&quot;LAST_MODIFICATION&quot;, &quot;&quot;);

writeToLog(&quot;m&quot;, &quot;Override start time: &quot; + overrideStartTime);
writeToLog(&quot;m&quot;, &quot;Last modification: &quot; + lastModificationTime);


if (overrideStartTime.length == 19) {
	startDateTime = str2date(overrideStartTime, &quot;yyyy-MM-dd HH:mm:ss&quot;);
}
else if (lastModificationTime.length &gt;= 19) {
