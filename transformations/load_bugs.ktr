           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>bugs</connection>
    <sql>SELECT
  1 AS bugs_const
, bug_id
, assignee.login_name AS assigned_to
, reporter.login_name AS reported_by
, comp.name AS component
, keywords
, op_sys AS opsys
, priority
, prod.name AS product
, CASE WHEN resolution = &apos;&apos; THEN &apos;&lt;empty&gt;&apos; ELSE resolution END AS resolution
, bug_severity AS severity
, bug_status AS status
, CASE WHEN status_whiteboard = &apos;&apos; THEN &apos;&lt;empty&gt;&apos; ELSE status_whiteboard END AS status_whiteboard
, CASE WHEN target_milestone = &apos;&apos; THEN &apos;&lt;empty&gt;&apos; ELSE target_milestone END AS target_milestone
, version
, bugs.creation_ts
, CAST(DATE(bugs.creation_ts)+0 AS SIGNED INTEGER) AS created_on
, CAST(TIME(bugs.creation_ts)+0 AS SIGNED INTEGER) AS created_at
, ${CF_FLAGS_SQL_PARAMETER} as flags_cf
FROM bugs
JOIN profiles assignee ON assignee.userid = bugs.assigned_to
JOIN profiles reporter ON reporter.userid = bugs.reporter
JOIN products prod ON prod.id = product_id
JOIN components comp ON comp.id = component_id
LEFT JOIN (
    SELECT
    bug_id AS keywords_bug_id,
    GROUP_CONCAT(`keyworddefs`.`name`) AS keywords
    FROM `keywords`
    LEFT JOIN `keyworddefs` ON `keywords`.`keywordid` = `keyworddefs`.`id`
) AS `kw` ON kw.keywords_bug_id = bug_id
WHERE delta_ts &gt; ?
AND ${BUG_IDS_PARTITION}
GROUP BY bug_id
ORDER BY bug_id ASC</sql>
