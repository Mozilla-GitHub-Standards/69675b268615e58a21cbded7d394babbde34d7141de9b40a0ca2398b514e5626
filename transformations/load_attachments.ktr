  <step>
    <name>... and their bugs</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>bugs</connection>
    <sql>SELECT
    att.`attach_id` AS activity_attachment_id,
    -UNIX_TIMESTAMP(`bug_when`) AS sort_date,
    `modifier`.`login_name` AS modified_by,
    `bug_when`,
    field.`name` AS field_name,
    `removed`,
    `added`
FROM
    `sanitized_bugs_activity` activity
    JOIN `fielddefs` field ON activity.`fieldid` = field.`id`
    JOIN `profiles` modifier ON `who` = modifier.`userid`
    JOIN (
       SELECT 
           `attach_id`,
           `bug_id` AS `attachment_bug_id`,
		   `creation_ts`
       FROM attachments
       WHERE ${BUG_IDS_PARTITION}
    ) AS att ON (`bug_id` = `attachment_bug_id` AND att.creation_ts &lt; bug_when)
WHERE
    `bug_when` &gt; &apos;${START_TIME}&apos;
    AND ${BUG_IDS_PARTITION}
    AND (
        field.`name` = &apos;bug_group&apos;
    )
ORDER BY
    activity_attachment_id,
    sort_date,
    modified_by</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
