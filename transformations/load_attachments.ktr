, ispatch AS is_patch
, isobsolete AS is_obsolete
, isurl AS is_url
, &apos;unknown&apos; AS mimetype
, attachments.bug_id
, submitter.login_name AS submitted_by
, attachments.creation_ts AS submission_ts
, CAST(DATE(attachments.creation_ts)+0 AS SIGNED INTEGER) AS created_on
, CAST(TIME(attachments.creation_ts)+0 AS SIGNED INTEGER) AS created_at
FROM attachments
JOIN profiles submitter ON submitter.userid = attachments.submitter_id
WHERE 
  modification_time &gt; &apos;${START_TIME}&apos;
