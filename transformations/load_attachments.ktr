    <connection>bugs</connection>
    <sql>SELECT
    activity.`attach_id` AS activity_attachment_id,
    `modifier`.`login_name` AS modified_by,
    `bug_when`,
    IF(`name` = &apos;flagtypes.name&apos;, &apos;flags&apos;, field.`name`) AS field_name,
    CASE
        WHEN `name` = &apos;flags&apos; AND `removed` IS NULL THEN &apos;&apos;
        ELSE NULLIF(`removed`, &apos;&apos;)
    END AS removed,
    CASE
        WHEN `name` = &apos;flags&apos; AND `added` IS NULL THEN &apos;&apos;
        ELSE NULLIF(`added`, &apos;&apos;)
    END AS added,
    CAST(DATE(`bug_when`)+0 AS SIGNED INT) AS modified_on,
    CAST(TIME(`bug_when`)+0 AS SIGNED INT) AS modified_at
FROM
    `sanitized_bugs_activity` activity
    JOIN `attachments` attachments ON activity.`attach_id` = attachments.`attach_id`
    JOIN `fielddefs` field ON activity.`fieldid` = field.`id`
    JOIN `profiles` modifier ON `who` = modifier.`userid`
WHERE
    NOT attachments.isprivate
    AND `bug_when` &gt; ?
    AND ${BUG_IDS_PARTITION}
    AND field.`name` IN (
        &apos;flagtypes.name&apos;,
        &apos;attachments.isobsolete&apos;, 
        &apos;attachments.ispatch&apos;,
        &apos;attachments.mimetype&apos;,
        &apos;attachments.cf_fixed_in&apos;,
        &apos;attachments.isurl&apos;
    )
    AND activity.`attach_id` IS NOT NULL
ORDER BY
    `activity_attachment_id`,
    `bug_when` DESC,
    `modified_by`;
    </sql>
