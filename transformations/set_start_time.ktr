           </partitioning>
    <compatible>N</compatible>
    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>var startTime;
var lastModification = last_modification;
var overrideStartTime = getVariable(&quot;POVERRIDE_START_TIME&quot;, &quot;&quot;);

writeToLog(&quot;m&quot;, &quot;Override start time: &quot; + overrideStartTime);
writeToLog(&quot;m&quot;, &quot;Last modification: &quot; + lastModification);


if (overrideStartTime.length == 19) {
	startTime = str2date(overrideStartTime, &quot;yyyy-MM-dd HH:mm:ss&quot;);
}
else if (lastModification != null &amp;&amp; lastModification.length &gt;= 19) {
    var cal = javax.xml.bind.DatatypeConverter.parseDateTime(lastModification);
    startTime = cal.getTime();
    writeToLog(&quot;m&quot;, &quot;Last Modification: &quot; + startTime);
    startTime = dateAdd(startTime, &quot;mi&quot;, -1);
}
else {
    writeToLog(&quot;m&quot;, &quot;Using default start date.&quot;);
    startTime = str2date(&quot;1901-01-01&quot;, &quot;yyyy-MM-dd&quot;);
}
</jsScript_script>
      </jsScript>    </jsScripts>    <fields>      <field>        <name>startTime</name>
