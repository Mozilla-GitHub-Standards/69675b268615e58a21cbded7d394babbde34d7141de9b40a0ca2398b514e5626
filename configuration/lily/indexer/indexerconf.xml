<?xml version="1.0"?>
<indexer xmlns:betl="com.mozilla.bugzilla_etl">
  <records>
    <record matchNamespace="betl" matchName="Bug" matchVariant="" vtags="history"/>
  </records>

  <fields>
    <!-- com.mozilla.bugzilla_etl.base.Fields.Bug -->
    <field name="bug_id" value="betl:id"/>
    <field name="reported_by" value="betl:reported_by"/>
    <field name="creation_date" value="betl:creation_date"/>

    <!-- com.mozilla.bugzilla_etl.base.Fields.Version -->
    <field name="modified_by" value="betl:modified_by"/>
    <field name="modification_date" value="betl:modification_date"/>
    <field name="expiration_date" value="betl:expiration_date"/>
    <field name="annotation" value="betl:annotation"/>

    <!-- com.mozilla.bugzilla_etl.base.Fields.Facet -->
    <field name="assigned_to" value="betl:assigned_to"/>
    <field name="changes" value="betl:changes"/>
    <field name="component" value="betl:component"/>
    <field name="flags" value="betl:flags"/>
    <field name="keywords" value="betl:keywords"/>
    <field name="major_status" value="betl:major_status"/>
    <field name="major_status_last_changed_date" value="betl:major_status_last_changed_date"/>
    <field name="modified_fields" value="betl:modified_fields"/>
    <field name="opsys" value="betl:opsys"/>
    <field name="priority" value="betl:priority"/>
    <field name="product" value="betl:product"/>
    <field name="previous_major_status" value="betl:previous_major_status"/>
    <field name="previous_status" value="betl:previous_status"/>
    <field name="resolution" value="betl:resolution"/>
    <field name="severity" value="betl:severity"/>
    <field name="status" value="betl:status"/>
    <field name="status_last_changed_date" value="betl:status_last_changed_date"/>
    <field name="status_whiteboard" value="betl:status_whiteboard"/>
    <field name="status_whiteboard_items" value="betl:status_whiteboard_items"/>
    <field name="target_milestone" value="betl:target_milestone"/>
    <field name="version" value="betl:version"/>

    <!-- com.mozilla.bugzilla_etl.base.Fields.Measurement -->
    <field name="days_in_previous_major_status" value="betl:days_in_previous_major_status"/>
    <field name="days_in_previous_status" value="betl:days_in_previous_status"/>
    <field name="days_in_major_status" value="betl:days_in_major_status"/>
    <field name="days_in_status" value="betl:days_in_status"/>
    <field name="days_open_accumulated" value="betl:days_open_accumulated"/>
    <field name="times_reopened" value="betl:times_reopened"/>
    <field name="number" value="betl:number"/>
  </fields>

</indexer>
